    "for num in range(1042000,702648265):\n",
    "    temp=num\n",
    "    order = len(str(num))\n",
    "    sum=0\n",
    "    while temp>0:\n",
    "        digit=temp%10\n",
    "        sum+=digit**order\n",
    "        temp//=10\n",
    "    if num==sum:\n",
    "        print(\"The First Armstrong Number in a given range : \",sum)\n",
    "        break"
   ]
  }
